{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client-dev": "cd client yarn start",
    "client-build": "cd client yarn build",
    "server-dev": "nodemon server.js",
    "server-build": "node server.js &",
    "dev": "concurrently -r -k \"yarn run server-dev\" \"yarn run client-dev\"",
    "build": "yarn; yarn --cwd 'client'; yarn run server-build; yarn run client-build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "response-time": "^2.3.2"
  }
}
