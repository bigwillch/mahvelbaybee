{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client-dev": "cd client && yarn start",
    "client-build": "cd client && yarn start",
    "server-dev": "nodemon server.js",
    "server-build": "node server.js &",
    "dev": "concurrently --kill-others-on-fail \"yarn server-dev\" \"yarn client-dev\"",
    "build": "yarn; yarn --cwd 'client'; concurrently --kill-others-on-fail \"yarn server-build\" \"yarn client-build\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
